<h2>Listing vehicles for <%= @driver.name %></h2>

<table class="table">
  <thead>
    <tr>
      <th>License Plate</th>
      <th>Kind</th>
      <th>Brand</th>
    </tr>
  </thead>
  <tbody>
<%= for vehicle <- @vehicles do %>
    <tr>
      <td><%= vehicle.license_plate %></td>
      <td><%= vehicle.kind %></td>
      <td><%= vehicle.brand %></td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New vehicle", to: driver_vehicle_path(@conn, :new, @driver) %>
